#!/bin/bash

. "${0%/*}/completer-testbase.bash"

# ==============================================================================
# Start tests
# ==============================================================================

test_cargo() {
  COMPLETER_IGNORE_CASE=1 assert_comp run_ruby -x $compdir/completer-cargo.rb -c "$@"
}

test_cargo 1 cargo <<'EOF'
'bench '
'build '
'check '
'clean '
'--color '
'doc '
'--explain '
'--frozen '
'-h '
'--help '
'help '
'init '
'install '
'--list '
'--locked '
'new '
'publish '
'-q '
'--quiet '
'run '
'search '
'test '
'update '
'-v '
'-V '
'--verbose '
'--version '
'-vv '
EOF

done_testing

#!/bin/bash

. mutil.sh

if [[ "$CHROME_REMOTE_DESKTOP_SESSION" != "" ]] ; then
	exit 0
fi

if [[ $(hostname -s) == "omakoto-ct1" ]] ; then
	exit 0
fi



if [[ -n "$SSH_AGENT_PID" ]] ; then
	echo "SSH_AGENT_PID already set." 1>&2
	exit 0
fi

saved=$(ramtmp)/ssh-agent-vars

if [[ -f $saved ]] ; then
	cat $saved
	exit 0
fi

ssh-agent -s | tee $saved
exit 0
